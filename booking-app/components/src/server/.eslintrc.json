{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb-base"],
  "rules": {
    "quotes": ["error", "double"],
    "camelcase": "warn",
    "import/prefer-default-export": "warn",
    "import/no-extraneous-dependencies": "warn",
    "prefer-object-spread": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
}
